<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0B1133"
    tools:context=".MenuPrincipal">

    <TextView
        android:id="@+id/t8.principal"
        android:layout_width="249dp"
        android:layout_height="52dp"
        android:layout_marginStart="76dp"
        android:layout_marginEnd="141dp"
        android:background="#F48FB1"
        android:text="Menu principal"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="117dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/creareven"
        android:layout_width="153dp"
        android:layout_height="51dp"
        android:layout_marginStart="68dp"
        android:layout_marginBottom="20dp"
        android:text="CrearEventos"
        app:layout_constraintBottom_toTopOf="@+id/creardocus"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/creardocus"
        android:layout_width="152dp"
        android:layout_height="51dp"
        android:layout_marginStart="68dp"
        android:layout_marginBottom="32dp"
        android:text="CrearDocumentos"
        app:layout_constraintBottom_toTopOf="@+id/calendario"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/calendario"
        android:layout_width="147dp"
        android:layout_height="48dp"
        android:layout_marginStart="76dp"
        android:layout_marginEnd="199dp"
        android:layout_marginBottom="41dp"
        android:text="Calendario"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/close"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/close"
        android:layout_width="150dp"
        android:layout_height="54dp"
        android:layout_marginStart="77dp"
        android:layout_marginEnd="197dp"
        android:layout_marginBottom="174dp"
        android:text="Cerrar sesion"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>